{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": "off",
        "no-unused-vars":"off",
        "no-debugger": "off",
        "max-len": "off",
        "no-else-return": "off"
    }
}
